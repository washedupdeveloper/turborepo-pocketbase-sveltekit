    "@playwright/test": "^1.31.2",
    "@sveltejs/adapter-auto": "latest",
    "@sveltejs/kit": "latest",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.51.3",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.1",
